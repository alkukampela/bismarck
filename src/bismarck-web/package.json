{
  "scripts": {
    "develop": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel ./src/index.html",
    "build": "npm run type-check && parcel build ./src/index.html --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.0.0"
  },
  "browserslist": [
    "> 1%",
    "not IE 11"
  ],
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "@parcel/transformer-mdx": "^2.0.1",
    "@parcel/transformer-typescript-tsc": "^2.0.1",
    "@testing-library/jest-dom": "^5.16.0",
    "@types/jest": "^26.0.24",
    "@types/mdx": "^2.0.1",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^6.4.0",
    "csstype": "^3.0.10",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.1",
    "parcel": "^2.0.1",
    "prettier": "^2.5.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-test-renderer": "^17.0.2",
    "reconnecting-websocket": "^4.4.0",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  }
}
